class HelloWorld {
    void leader(int arr[], int n){
        int lead= arr[n-1];
        System.out.print(lead + " ");
        for(int i=n-2; i>= 0; i--){
            if(lead<= arr[i]){
                lead= arr[i];
                
        System.out.println(lead+" ");
            }
        }
        
    }
    public static void main(String[] args){
        HelloWorld ob= new HelloWorld();
        int arr[] = {16, 17, 4, 3, 5, 2};
        int n=arr.length;
        ob.leader(arr, n);
    }
}
